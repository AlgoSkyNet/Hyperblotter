{
    "startup_app": {
        "name": "Hyperblotter",
        "description": "Hyperblotter Demo",
        "url": "http://cdn.openfin.co/demos/hyperblotter-bloomberg/index.html",
        "__url": "http://localhost:5001/index.html?debug",
        "icon": "http://cdn.openfin.co/demos/hyperblotter-bloomberg/images/hyperblotter-bloomberg.ico",
        "__icon": "http://localhost:5001/images/hyperblotter-bloomberg.ico",
        "uuid": "hyperblotter",
        "autoShow": true,
        "defaultWidth": 420,
        "maxWidth": 420,
        "minWidth": 420,
        "maxHeight": 90,
        "defaultHeight": 90,
        "minHeight": 90,
        "defaultTop": 50,
        "defaultLeft": 10,
        "resizable": false,
        "maximizable": false,
        "frame": false,
        "alwaysOnTop":true,
        "cornerRounding": {
            "width": 5,
            "height": 5
        }
    },
    "runtime": {
        "arguments": "",
        "version": "beta"
    },
    "appAssets": [
        {
            "src": "http://cdn.openfin.co/demos/hyperblotter-bloomberg/assets/dist.zip",
            "__src": "http://localhost:5001/assets/dist.zip",
            "alias": "excel-dist",
            "target": "excel.exe",
            "version": "0.0.1",
            "args" : "-i -l hyperblotter.xlsx"
        },
        {
            "src": "http://cdn.openfin.co/demos/hyperblotter-bloomberg/assets/HyperblotterChart.zip",
            "__src": "http://localhost:5001/assets/HyperblotterChart.zip",
            "alias": "hyperblotter-chart",
            "target": "HyperblotterChart.exe",
            "version": "1.0.0"
        },
        {
            "src": "http://cdn.openfin.co/demos/hyperblotter-bloomberg/assets/openfin-plugin-bloomberg.zip",
            "alias": "bloomberg",
            "target": "server.bat",
            "version": "0.0.1"
        }
    ],
    "splashScreenImage": "http://cdn.openfin.co/demos/hyperblotter-bloomberg/images/splashscreen.png",
    "__splashScreenImage": "http://localhost:5001/images/splashscreen.png",
    "shortcut": {
        "company": "OpenFin",
        "description": "Hyperblotter Bloomberg",
        "icon": "http://cdn.openfin.co/demos/hyperblotter-bloomberg/images/hyperblotter-bloomberg.ico",
        "__icon": "http://localhost:5001/images/hyperblotter-bloomberg.ico",
        "name": "Hyperblotter Bloomberg"
    }
}
