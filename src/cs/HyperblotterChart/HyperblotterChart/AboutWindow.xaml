<Window x:Class="ChartIQDemo.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChartIQDemo"
        mc:Ignorable="d"
        Title="About" Height="260" Width="360" ResizeMode="NoResize" WindowStartupLocation="CenterOwner" VerticalContentAlignment="Stretch">
    <DockPanel Margin="10">
        <Button DockPanel.Dock="Bottom" Width="100" VerticalAlignment="Bottom" Click="Button_Click">OK</Button>
        <TextBlock x:Name="aboutContent" DockPanel.Dock="Top" TextWrapping="Wrap" VerticalAlignment="Top"></TextBlock>
    </DockPanel>
</Window>
